<div id='map'></div>
<script is:inline>
  let map

  function initMap() {
    // map = new google.maps.Map(document.getElementById('map'), {
    //   center: { lat: -34.397, lng: 150.644 },
    //   zoom: 8,
    // })
    const myLatLng = { lat: 42.69135607193375, lng: 23.32145416142027 }

    const map = new google.maps.Map(document.getElementById('map'), {
      zoom: 14,
      center: myLatLng,
    })

    new google.maps.Marker({
      position: myLatLng,
      map,
      title: 'Punkt Studio!',
    })
  }

  window.initMap = initMap
</script>
<script
  is:inline
  src='https://maps.googleapis.com/maps/api/js?key=AIzaSyBF5VoJ54Kz4IC4WgGQC1kIF2WW4tH4pv4&callback=initMap&v=weekly'
  defer
></script>
<style>
  #map {
    width: 100%;
    min-height: 10rem;
  }
</style>
